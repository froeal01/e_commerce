
		
			 <%  if flash[:notice] %>
					<div class = "uk-alert-success uk-alert" data-uk-alert>
						<a href="" class= "uk-alert-close uk-close"></a>
						<p><%=  flash[:notice] %></p>
					</div>
				<%  else %>
					<div class = "uk-alert-warning uk-alert" data-uk-alert>
						<a href="" class= "uk-alert-close uk-close"></a>
						<p><%=  flash[:alert] %></p>
					</div>
				<%  end %>
		</div>
